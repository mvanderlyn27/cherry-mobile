{
  "name": "expo-app-boilerplate",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "build:dev": "eas build --profile development --platform all --local",
    "build:dev-ios": "eas build --profile development --platform ios --local --output=./builds/ios/development.ipa",
    "build:dev-android": "eas build --profile development --platform android --local --output=./builds/android/development.apk",
    "build:preview": "eas build --profile preview --platform all --local",
    "build:preview-ios": "eas build --profile preview --platform ios --local --output=./builds/ios/preview.ipa",
    "build:preview-android": "eas build --profile preview --platform android --local --output=./builds/android/preview.apk",
    "build:production": "eas build --profile production --platform all --local",
    "build:production-ios": "eas build --profile production --platform ios --local --output=./builds/ios/production.ipa",
    "build:production-android": "NODE_ENV=production APP_ENV=production eas build --profile production --platform android --local --output=./builds/android/production.aab",
    "submit:ios": "eas submit -p ios --path ./builds/ios/production.ipa",
    "submit:android": "eas submit -p android --path ./builds/android/production.aab",
    "prebuild": "node -e \"require('fs').copyFileSync('.env.' + process.env.APP_ENV, '.env')\"",
    "syncDb:local": "DOTENV_CONFIG_PATH=.env.local bunx tsx -r dotenv/config npx @snaplet/seed sync",
    "syncDb:dev": "DOTENV_CONFIG_PATH=.env.development bunx tsx -r dotenv/config npx @snaplet/seed sync",
    "genTestUsers": "DOTENV_CONFIG_PATH=.env.local bunx tsx -r dotenv/config ./supabase/testUsers.ts",
    "genSeedData": "DOTENV_CONFIG_PATH=.env.local bunx tsx -r dotenv/config ./supabase/seed.ts"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo-google-fonts/heebo": "^0.2.3",
    "@expo-google-fonts/kaisei-decol": "^0.2.3",
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/bottom-sheet": "^5.1.2",
    "@legendapp/list": "^1.0.0-beta.24",
    "@legendapp/state": "^3.0.0-beta.30",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/slider": "4.5.5",
    "@react-native-google-signin/google-signin": "^13.2.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.49.1",
    "@superwall/react-native-superwall": "^1.4.7",
    "add": "^2.0.6",
    "bun": "^1.2.5",
    "eas-cli": "^16.1.0",
    "expo": "~52.0.35",
    "expo-apple-authentication": "~7.1.3",
    "expo-blur": "~14.0.3",
    "expo-build-properties": "~0.13.2",
    "expo-constants": "~17.0.6",
    "expo-dev-client": "~5.0.12",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image": "~2.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.8",
    "expo-updates": "~0.27.4",
    "expo-web-browser": "~14.0.2",
    "moti": "^0.30.0",
    "nativewind": "^4.1.23",
    "posthog-react-native": "^3.12.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-drawer-layout": "^4.1.2",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-mmkv": "^3.2.0",
    "react-native-purchases": "^8.9.4",
    "react-native-purchases-ui": "^8.9.4",
    "react-native-reanimated": "~3.16.1",
    "react-native-reanimated-carousel": "^4.0.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-toast-message": "^2.2.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-uuid": "^2.0.3",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "supabase": "^2.19.7",
    "tailwindcss": "^3.4.17",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@snaplet/copycat": "^6.0.0",
    "@snaplet/seed": "0.98.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^18.3.0",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.4",
    "postgres": "^3.4.5",
    "react-test-renderer": "18.3.1",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3"
  },
  "nativewind": {
    "tailwindConfig": "./tailwind.config.js"
  },
  "private": true
}
